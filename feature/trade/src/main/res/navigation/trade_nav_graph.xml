<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/trade_nav_graph"
    app:startDestination="@id/tradeListFragment">

    <fragment
        android:id="@+id/tradeListFragment"
        android:name="com.renatsayf.trade.lists.TradeListFragment"
        android:label="fragment_trade_list"
        tools:layout="@layout/fragment_trade_list" >
        <action
            android:id="@+id/action_tradeListFragment_to_profile_nav_graph"
            app:destination="@id/profile_nav_graph" />
    </fragment>
    <include app:graph="@navigation/profile_nav_graph" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.renatsayf.profile.ProfileFragment"
        android:label="ProfileFragment" />
</navigation>